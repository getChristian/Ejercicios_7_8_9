package Ejercicios;

import java.util.ArrayList;

public class arrayConBucle {

    public static void main(String[] args) {

        ArrayList<Integer> list = new ArrayList<Integer>();

        for (int i = 1; i < 11; i++){
            list.add(i);

            for(int num = 0; num < list.size();num++){

                if(list.get(num) % 2 == 0){
                    list.remove(num);
                }
            }
        }

        System.out.println(list);
    }
}
